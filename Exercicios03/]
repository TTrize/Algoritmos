#include <stdio.h>

int main(void){


  float fulano = 1.5, ciclano = 1.1;
  int i, anos;
 
  while(ciclano < fulano){
  i++;
  anos = anos + i;
  ciclano += 0.03;
  fulano += 0.02;

  printf("\nfulano %.2f\n", fulano);
  printf("\nciclano %.2f\n", ciclano);
  }
  printf("\nAnos: %d\n", anos);
  return 0;
}

